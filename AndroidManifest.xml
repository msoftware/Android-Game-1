<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pukku.accelero"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.pukku.accelero.StartActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.pukku.accelero.MenuActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MENUACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.pukku.accelero.Level1Activity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.LEVEL1ACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.pukku.accelero.Level2Activity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.LEVEL2ACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.pukku.accelero.Level3Activity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.LEVEL3ACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.pukku.accelero.OptionsActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.OPTIONSACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.pukku.accelero.TryAgain"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.TRYAGAINACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.pukku.accelero.Help"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.HELPACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.pukku.accelero.LevelSelector"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.LEVELSELECTOR" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>